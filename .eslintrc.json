{
  "env": {
    "browser": true,
    "commonjs": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "eol-last": ["error", "always"],
      "no-console": "off"
  }
}
