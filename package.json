{
  "name": "doggy-names",
  "version": "1.0.0",
  "description": "Funny library to get random doggy names",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "nyc mocha",
    "test:watch": "mocha --watch",
    "prebuild": "rimraf dist",
    "build": "cpx src/index.js dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ezavile/doggy-names.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dog",
    "doggy",
    "pet",
    "dog name"
  ],
  "author": "Edgar Zavala <ezavile@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ezavile/doggy-names/issues"
  },
  "homepage": "https://github.com/ezavile/doggy-names#readme",
  "devDependencies": {
    "chai": "4.1.2",
    "cpx": "1.5.0",
    "eslint": "4.15.0",
    "mocha": "5.0.0",
    "nyc": "11.4.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "pre-commit": [
    "test"
  ],
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
